public class Student {

	private int StuNum;
	private String name;
	
	public int getStuNum() {
		return StuNum;
	}
	public void setStuNum(int stuNum) {
		StuNum = stuNum;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
	public void TellUs(){
		System.out.println("我的学号是："+StuNum+",我的名字是："+name);
	}
}


public class TestStudent {

	public static void main(String args[]){
		Student stu1 = new Student();
		
		stu1.setStuNum(123);
		stu1.setName("feiqiu");
		System.out.println(stu1.getName()+":"+stu1.getStuNum());
		stu1.TellUs();
	}
}

//深入理解Java虚拟机（周志明）
